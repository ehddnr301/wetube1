{
  "name": "wetube",
  "version": "1.0.0",
  "main": "init.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "tsc-watch --onSuccess \" node build/init.js\" ",
    "dev:assets": "WEBPACK_ENV=development TS_NODE_PROJECT=\"./tsconfig-for-webpack-config.json\" webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore \"src/assets\" --extensions \".ts,.tsx\"",
    "prebuild": "rm -rf build",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
